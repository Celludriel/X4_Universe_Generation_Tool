    <xs:attribute name="selectable" type="booleanexpression">
      <xs:annotation>
        <xs:documentation>
          Optional, determines whether the option is selectable. Defaults to true if not supplied.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tooltip" type="expression">
